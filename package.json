{
  "name": "angular-app",
  "version": "0.0.1",
  "private": false,
  "scripts": {
    "start": "ng serve",
    "check": "npm run lint && npm run test",
    "test": "ng test --code-coverage",
    "test:watch": "ng test --code-coverage --watch",
    "test:debug": "ng test --code-coverage --watch --browsers=ChromeDebugging",
    "lint": "prettier --check \"src/**/*.{ts,scss,html}\"",
    "lint:fix": "prettier --write \"src/**/*.{ts,scss,html}\"",
    "clean": "rimraf dist/ .angular/",
    "build": "ng build"
  },
  "dependencies": {
    "@angular/animations": "13.2.1",
    "@angular/common": "13.2.1",
    "@angular/compiler": "13.2.1",
    "@angular/core": "13.2.1",
    "@angular/forms": "13.2.1",
    "@angular/platform-browser": "13.2.1",
    "@angular/platform-browser-dynamic": "13.2.1",
    "@angular/router": "13.2.1",
    "@ngrx/effects": "13.1.0",
    "@ngrx/router-store": "13.1.0",
    "@ngrx/store": "13.1.0",
    "observable-webworker": "4.0.1",
    "rxjs": "7.5.0",
    "tslib": "2.3.0",
    "zone.js": "0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.2.1",
    "@angular/cli": "~13.2.1",
    "@angular/compiler-cli": "~13.2.1",
    "@ngrx/schematics": "^13.1.0",
    "@ngrx/store-devtools": "^13.1.0",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^14.18.2",
    "jasmine-core": "~3.10.0",
    "jasmine-marbles": "^0.9.2",
    "karma": "^6.3.9",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.1.0",
    "karma-jasmine": "^4.0.1",
    "karma-jasmine-html-reporter": "^1.7.0",
    "ng-mocks": "^13.5.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.4.0",
    "typescript": "~4.5.2"
  }
}
